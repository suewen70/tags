{
  "name": "vanilla-parcel-test-task",
  "version": "1.0.0",
  "description": "simple vanilla JS apps",
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && parcel public/index.html --out-dir development -p 3000",
    "build": "rimraf ./dist && parcel build public/*.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf ./.cache"
  },
  "keywords": [
    "parcel"
  ],
  "author": "Sue",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "materialize-css": "^1.0.0-rc.2",
    "popper.js": "^1.16.1"
  }
}
